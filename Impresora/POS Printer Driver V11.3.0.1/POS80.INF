[Version]
Signature="$Windows NT$"
Provider=%AUTHOR%
LayoutFile=ntprint.inf
ClassGUID={4D36E979-E325-11CE-BFC1-08002BE10318}
Class=Printer
DriverVer=08/09/2010
CatalogFile=POS2.cat

[ClassInstall32.NT]
AddReg=printer_class_addreg

[printer_class_addreg]
HKR,,,,%PrinterClassName%
HKR,,Icon,,"-4"
HKR,,Installer32,,"ntprint.dll,ClassInstall32"
HKR,,NoDisplayClass,,1
HKR,,EnumPropPages32,,"printui.dll,PrinterPropPageProvider"

[Manufacturer]
"POS"=POS, NTx86, NTamd64, NTia64

[POS]
"POS-80 11.3.0.1"      = DRIVER_POS80

[POS.NTx86]
"POS-80 11.3.0.1"      = DRIVER_POS80

[POS.NTamd64]
"POS-80 11.3.0.1"      = DRIVER_POS80

[POS.NTia64]
"POS-80 11.3.0.1"      = DRIVER_POS80

[DRIVER_POS80]
CopyFiles=@pos80.ini
CopyFiles=@POS80.GPD
CopyFiles=POS80FILES
DataSection=UNIDRV_DATA
DataFile=POS80.GPD
Include=NTPRINT.INF
Needs=UNIDRV.OEM
;Addreg = port_AddReg

;[DefaultInstall.NT]
;[ClassInstall32.ntx86]
;[ClassInstall32]

;[port_AddReg]
;HKLM,"SOFTWARE\Microsoft\Windows NT\CurrentVersion\Ports","COM1:",0x00000000,%COMMCONFIG%
;HKLM,"SOFTWARE\Microsoft\Windows NT\CurrentVersion\Ports","COM2:",0x00000000,%COMMCONFIG%
;HKLM,"SOFTWARE\Microsoft\Windows NT\CurrentVersion\Ports","COM3:",0x00000000,%COMMCONFIG%
;HKLM,"SOFTWARE\Microsoft\Windows NT\CurrentVersion\Ports","COM4:",0x00000000,%COMMCONFIG%

[POS80FILES]
pos80d.dll
RES80.DLL

[SourceDisksNames]
100 = %disk1%

[SourceDisksFiles.x86]
pos80d.dll = 100,i386
RES80.DLL =  100,i386

[SourceDisksFiles.amd64]
pos80d.dll = 100,x64
RES80.DLL =  100,x64

[SourceDisksFiles.ia64]
pos80d.dll   = 100,ia64
RES80.DLL =  100,ia64

[DestinationDirs]
DefaultDestDir=66000

;[ControlFlags]
;ExcludeFromSelect = 2369

[Strings]
AUTHOR="Microsoft"
COMMCONFIG="9600,n,8,1,p"
disk1="POS80 DRIVER"
